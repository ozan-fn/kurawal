{
    "version": 2,
    "buildCommand": "cd backend && npm run build",
    "builds": [
        {
            "src": "backend/dist/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/dist/server.js"
        }
    ]
}
